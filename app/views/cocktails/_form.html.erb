<%= simple_form_for(@cocktail) do |f| %>
 <%= f.error_notification %>
 <div class='row'>
 <div class="form-inputs col-xs-6 col-md-6 col-lg-6 col-md-offset-3">
   <%= f.input :name %><br>
   <p><%= f.input :photo, as: :attachinary %></p>
   <%= f.button :submit, class: "btn btn-primary" %>
 </div>
 </div>
<% end %>
